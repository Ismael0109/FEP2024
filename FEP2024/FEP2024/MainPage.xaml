<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="FEP2024.MainPage">

    <StackLayout>
        <!-- a função CLICKED do Button para abrir a tela que você está programando. 
        Ao final reformaremos essa parte, mas por enquanto utilize o button com seu nome para abrir sua parte, 
        caso ainda não tenha a parte que abre sua tela-->
        <Button Text="Ana Elisa" x:Name="btAna" Clicked="btAna_Clicked"/>

        <Button Text="Ismael" x:Name="btIsmael" Clicked="btIsmael_Clicked"/>
            
        <Button Text="Karol" x:Name="btKarol" Clicked="btKarol_Clicked"/>
            
        <Button Text="Luisa" x:Name="btLuisa" Clicked="btLuisa_Clicked"/>
            
        <Button Text="Miranda" x:Name="btMi" Clicked="btMi_Clicked"/>
       
    </StackLayout>

</ContentPage>
